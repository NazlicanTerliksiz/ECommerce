<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.profile.ProfileFragment"
    android:background="@color/bej">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/cartProductToolbar"
        android:layout_width="match_parent"
        app:title="My Profile"
        app:titleTextColor="@color/coffe"
        android:layout_height="wrap_content"
        android:background="@color/bej"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/EmailInfoText"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:layout_margin="16dp"
        android:text="TextView"
        android:textSize="16sp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cartProductToolbar"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/logOut"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:layout_margin="16dp"
        android:text="Log Out"
        android:textSize="16sp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/EmailInfoText"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>

